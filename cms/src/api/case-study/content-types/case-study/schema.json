{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case Study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "excerpt": {
      "type": "string"
    },
    "hero_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technology.technology",
      "inversedBy": "case_studies"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "case_studies"
    },
    "detail_content": {
      "type": "dynamiczone",
      "components": [
        "media.image-gallery",
        "button.link-button",
        "grid.grid-row",
        "heading.content-title",
        "description.content-description",
        "grid.grid-row-2",
        "grid.grid-card-row",
        "grid.grid-card-row-2"
      ]
    }
  }
}
